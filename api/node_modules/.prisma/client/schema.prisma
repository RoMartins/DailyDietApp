// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Meal {
  id          String   @id @default(uuid())
  inTheDiet   Boolean
  name        String
  description String
  created_at  DateTime @default(now())
}
